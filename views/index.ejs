  <body>
    <!-- Header -->

    <section id="header">
      <div class="header container">
        <div class="nav-bar">
          <div class="brand">
            <a href="#hero">
              <h1><span> Vplus</span></h1>
            </a>
          </div>
          <div class="nav-list">
            <div class="hamburger">
              <div class="bar"></div>
            </div>
            <ul>
              <li><a href="/" data-after="Home">Home</a></li>
              <li><a href="#about" data-after="About">About</a></li>
              <li><a href="#contact" data-after="Contact">Contact</a></li>
              <li>
                <a href="/login" data-after="Projects">Admin Login</a>
              </li>
            </ul>
          </div>
        </div>
      </div>
    </section>
    <!-- End Header -->

    <!-- Hero Section  -->
    <section id="hero">
      <div class="hero container">
        <div>
          <h1>Welcome to<span></span></h1>
          <h1>IIIT Allahabad <span></span></h1>
          <h1>Campus<span></span></h1>
          <a href="/visitor" type="button" class="cta">Visit Us</a>
        </div>
      </div>
    </section>
    <!-- End Hero Section  -->

    <!-- About Section -->
    <section id="about">
      <div class="about container">
        <div class="col-left">
          <div class="about-img">
            <img src="img/logo.gif" alt="img" />
          </div>
        </div>
        <div class="col-right">
          <h1 class="section-title">About <span>IIITA</span></h1>
          <br />
          <p>
            Indian Institute of Information Technology, Allahabad is a public
            university <br />
            located in Jhalwa, Allahabad,in Uttar Pradesh. It is one of the
            nineteen <br />
            Indian Institutes of Information Technology listed <br />
            by the Ministry of Education.
          </p>
          <!-- <a href="#" class="cta">Download Resume</a> ---->
        </div>
      </div>
    </section>
    <!-- End About Section -->

    <!-- Contact Section -->
    <section id="contact">
      <div class="contact container">
        <div>
          <h1 class="section-title">
            <b>Contact <span>info</span></b>
          </h1>
          <br />
        </div>
        <div class="contact-items">
          <div class="contact-item">
            <div class="icon">
              <img src="https://img.icons8.com/bubbles/100/000000/phone.png" />
            </div>
            <div class="contact-info">
              <h1>Phone</h1>
              <h2>+91-532-2922000</h2>
              <h2>+91-234-1231234</h2>
            </div>
          </div>
          <div class="contact-item">
            <div class="icon">
              <img
                src="https://img.icons8.com/bubbles/100/000000/new-post.png"
              />
            </div>
            <div class="contact-info">
              <h1>Email</h1>
              <h2>contact@iiita.ac.in</h2>
              <h2>admin@iiita.ac.in</h2>
            </div>
          </div>
          <div class="contact-item">
            <div class="icon">
              <img
                src="https://img.icons8.com/bubbles/100/000000/map-marker.png"
              />
            </div>
            <div class="contact-info">
              <h1>Address</h1>
              <h2>Devghat, Jhalwa, Prayagraj-211015, U. P. INDIA</h2>
            </div>
          </div>
        </div>
      </div>
    </section>
    <!-- End Contact Section -->

